<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>q1</servlet-name>
        <servlet-class>com.quiz.q1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>q2</servlet-name>
        <servlet-class>com.quiz.q2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Score</servlet-name>
        <servlet-class>com.quiz.Score</servlet-class>
    </servlet>
  
    <servlet>
        <servlet-name>ans1</servlet-name>
        <servlet-class>com.quiz.ans1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ans2</servlet-name>
        <servlet-class>com.quiz.ans2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>q1</servlet-name>
        <url-pattern>/q1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>q2</servlet-name>
        <url-pattern>/q2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Score</servlet-name>
        <url-pattern>/Score</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>ans1</servlet-name>
        <url-pattern>/ans1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ans2</servlet-name>
        <url-pattern>/ans2</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
